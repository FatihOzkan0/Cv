@using EntityLayer.Concrete
@model List<Education>

<div class="resume-section-content">
    <h2 class="mb-5">EĞİTİM</h2>

    @foreach (var item in Model)
    {


    <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
        <div class="flex-grow-1">
            <h3 class="mb-0"></h3>
            <div class="subheading mb-3">@item.Heading</div>
            <div>@item.SubHead1 - @item.SubHead2</div>
            <p>GNO: @item.Average</p>
        </div>
        <div class="flex-shrink-0"><span class="text-primary">Ekim 2020 - Temmuz 2024</span></div>
    </div>
    }

</div>