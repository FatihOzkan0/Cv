@using EntityLayer.Concrete
@model List<Experience> 


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>PROJELER</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>BAŞLIK</th>
        <th>ALT BAŞLIK</th>
        <th>AÇIKLAMA</th>
        <th>TARİH</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Heading</td>
            <td>@item.SubHead</td>
            <td>@item.Description</td>
            <td>@item.Date</td>
            <td><a href="/Experiences/ExperiencesDelete/@item.ID" class="btn btn-danger">Sil</a></td>
            <td><a href="/Experiences/ExperienceEdit/@item.ID" class="btn btn-success">Düzenle</a></td>
        </tr>
    }

</table>
<br />
<a href="/Experiences/ExperiencesAdd" class="btn btn-primary">Yeni Proje Ekle</a>

